{
  "name": "nodejs-starter-ts",
  "version": "1.0.0",
  "author": "Tanmay Mishu <tanmaymishu@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "migrate:make": "knex --knexfile src/database/knexfile.ts migrate:make -x ts",
    "migrate:latest": "knex --knexfile src/database/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback",
    "db:config": "cp src/database/knexfile.ts.example src/database/knexfile.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.6",
    "@types/node": "^15.12.5",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "nodemon": "^2.0.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "objection": "^2.2.15",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "winston": "^3.3.3"
  }
}
